<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="868.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.time.game.Controller.Profile.LevelStatsController">
   <children>
      <Text fx:id="statsPageTitle" layoutX="401.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Level 18 Progress Report">
         <font>
            <Font size="49.0" />
         </font>
      </Text>
      <LineChart fx:id="levelStatsGraph" layoutX="90.0" layoutY="185.0" prefHeight="565.0" prefWidth="1037.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="profileButton" layoutX="1186.0" layoutY="701.0" mnemonicParsing="false" onAction="#onProfileButtonClicked" text="Profile -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="1206.0" layoutY="750.0" mnemonicParsing="false" onAction="#onExitButtonClicked" text="Exit -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="playLevelButton" layoutX="1158.0" layoutY="554.0" mnemonicParsing="false" onAction="#onPlayLevelButtonClicked" text="Play Level -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="viewNextLevelButton" layoutX="714.0" layoutY="797.0" mnemonicParsing="false" onAction="#onViewNextLevelButtonClicked" text="View Next Level -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="viewPreviousLevelButton" layoutX="490.0" layoutY="797.0" mnemonicParsing="false" onAction="#onViewPreviousLevelButtonClicked" text="&lt;- View Previous Level">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="bpmSelector" layoutX="1227.0" layoutY="603.0" prefHeight="25.0" prefWidth="53.0" />
      <Text layoutX="1158.0" layoutY="620.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select BPM" />
   </children>
</AnchorPane>
