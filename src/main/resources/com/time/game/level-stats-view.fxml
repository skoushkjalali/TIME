<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="868.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.time.game.Controller.Profile.LevelStatsController">
   <children>
      <Text fx:id="statsPageTitle" layoutX="395.0" layoutY="118.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Level 18 Progress Report">
         <font>
            <Font size="49.0" />
         </font>
      </Text>
      <LineChart fx:id="levelStatsGraph" layoutX="45.0" layoutY="185.0" prefHeight="565.0" prefWidth="1052.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="profileButton" layoutX="1114.0" layoutY="719.0" mnemonicParsing="false" onAction="#onProfileButtonClicked" prefHeight="40.0" prefWidth="150.0" text="Profile -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="1114.0" layoutY="768.0" mnemonicParsing="false" onAction="#onExitButtonClicked" prefHeight="40.0" prefWidth="150.0" text="Exit -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="playLevelButton" layoutX="1114.0" layoutY="540.0" mnemonicParsing="false" onAction="#onPlayLevelButtonClicked" prefHeight="80.0" prefWidth="150.0" text="Play Level -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="viewNextLevelButton" layoutX="655.0" layoutY="750.0" mnemonicParsing="false" onAction="#onViewNextLevelButtonClicked" prefHeight="40.0" prefWidth="200.0" text="View Next Level -&gt;">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="viewPreviousLevelButton" layoutX="445.0" layoutY="750.0" mnemonicParsing="false" onAction="#onViewPreviousLevelButtonClicked" prefHeight="40.0" prefWidth="200.0" text="&lt;- View Previous Level">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="bpmSelector" layoutX="1191.0" layoutY="493.0" prefHeight="25.0" prefWidth="72.0" />
      <Text layoutX="1115.0" layoutY="510.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select BPM" />
   </children>
</AnchorPane>
