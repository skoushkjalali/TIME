<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="868.0" prefWidth="1300.0" stylesheets="@../../../CSS/LevelStatsStyle.css"
            xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.time.game.Controller.Profile.LevelStatsController">
   <Rectangle arcHeight="18.0" arcWidth="18.0" fill="#1e1e1e" height="132.0" layoutX="1097.0" layoutY="308.0"
              stroke="#d5a72a" strokeType="INSIDE" strokeWidth="0.75" width="182.0"/>
   <Rectangle arcHeight="18.0" arcWidth="18.0" fill="#1e1e1e" height="176.0" layoutX="1097.0" layoutY="455.0"
              stroke="BLACK" strokeType="INSIDE" width="182.0"/>
   <Rectangle arcHeight="18.0" arcWidth="18.0" fill="#1e1e1e" height="132.0" layoutX="1100.0" layoutY="643.0"
              stroke="BLACK" strokeType="INSIDE" width="182.0"/>
   <Text fx:id="statsPageTitle" fill="WHITE" layoutX="395.0" layoutY="118.0" strokeType="OUTSIDE" strokeWidth="0.0"
         text="Level 18 Progress Report">
      <font>
         <Font size="49.0"/>
      </font>
   </Text>
   <LineChart fx:id="levelStatsGraph" animated="false" layoutX="36.0" layoutY="177.0" prefHeight="616.0"
              prefWidth="1061.0">
      <xAxis>
         <CategoryAxis side="BOTTOM" fx:id="xAxis"/>
      </xAxis>
      <yAxis>
         <NumberAxis fx:id="yAxis" minorTickCount="0" side="LEFT"/>
      </yAxis>
   </LineChart>
   <Button fx:id="profileButton" layoutX="1117.0" layoutY="664.0" mnemonicParsing="false"
           onAction="#onProfileButtonClicked" prefHeight="89.0" prefWidth="150.0" text="Home">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <Button fx:id="playLevelButton" layoutX="1114.0" layoutY="526.0" mnemonicParsing="false"
           onAction="#onPlayLevelButtonClicked" prefHeight="80.0" prefWidth="150.0" text="Play Level">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <Button fx:id="viewNextLevelButton" layoutX="1113.0" layoutY="382.0" mnemonicParsing="false"
           onAction="#onViewNextLevelButtonClicked" prefHeight="40.0" prefWidth="150.0" text="View Next">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <Button fx:id="viewPreviousLevelButton" layoutX="1113.0" layoutY="327.0" mnemonicParsing="false"
           onAction="#onViewPreviousLevelButtonClicked" prefHeight="40.0" prefWidth="150.0" text="View Previous">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <ChoiceBox fx:id="bpmSelector" layoutX="1191.0" layoutY="479.0" prefHeight="25.0" prefWidth="72.0"/>
   <Text fill="WHITE" layoutX="1115.0" layoutY="496.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select BPM"/>
   <Text fill="WHITE" layoutX="529.0" layoutY="813.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Attempt">
      <font>
         <Font size="30.0"/>
      </font>
   </Text>
   <Text fx:id="scoreLabelText" fill="WHITE" layoutY="503.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score">
      <font>
         <Font size="30.0"/>
      </font>
   </Text>
</AnchorPane>
