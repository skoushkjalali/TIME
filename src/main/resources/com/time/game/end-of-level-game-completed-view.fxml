<AnchorPane prefHeight="868.0" prefWidth="1300.0" stylesheets="@../../../CSS/LastLevelStyle.css"
            xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.time.game.Controller.EndOfLevel.GameCompletedController">
   <Rectangle arcHeight="18.0" arcWidth="18.0" fill="WHITE" height="668.0" layoutX="299.0" layoutY="90.0" opacity="0.02"
              stroke="BLACK" strokeType="INSIDE" width="700.0"/>
   <Rectangle arcHeight="18.0" arcWidth="18.0" fill="#1e1e1e" height="87.0" layoutX="347.0" layoutY="622.0"
              stroke="#d5a72a" strokeType="INSIDE" strokeWidth="0.75" width="604.0"/>
   <Rectangle arcHeight="18.0" arcWidth="18.0" fill="#1e1e1e" height="134.0" layoutX="443.0" layoutY="453.0"
              stroke="BLACK" strokeType="INSIDE" width="409.0"/>
   <Rectangle arcHeight="18.0" arcWidth="18.0" fill="#1e1e1e" height="277.0" layoutX="350.0" layoutY="144.0"
              stroke="#d5a72a" strokeType="INSIDE" strokeWidth="0.0" width="604.0"/>
   <Button fx:id="profileButton" layoutX="580.0" layoutY="646.0" mnemonicParsing="false"
           onAction="#onProfileButtonClick" prefHeight="40.0" prefWidth="150.0" text="Home">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <Button fx:id="exitButton" layoutX="771.0" layoutY="646.0" mnemonicParsing="false" onAction="#onExitButtonClick"
           prefHeight="40.0" prefWidth="150.0" text="Close App">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <Button fx:id="repeatLevelButton" layoutX="680.0" layoutY="480.0" mnemonicParsing="false"
           onAction="#onRepeatLevelButtonClicked" prefHeight="80.0" prefWidth="150.0" text="Repeat Level">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <Text fx:id="scoreText" fill="WHITE" layoutX="802.0" layoutY="301.0" strokeType="OUTSIDE" strokeWidth="0.0"
         text="100%" textAlignment="RIGHT">
      <font>
         <Font size="49.0"/>
      </font>
   </Text>
   <Text fill="WHITE" layoutX="-7.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Final Level Completed"
         textAlignment="CENTER" wrappingWidth="1300.0">
      <font>
         <Font size="49.0"/>
      </font>
   </Text>
   <Text fx:id="levelHighScoreText" fill="WHITE" layoutX="802.0" layoutY="383.0" strokeType="OUTSIDE" strokeWidth="0.0"
         text="100%" textAlignment="RIGHT">
      <font>
         <Font size="49.0"/>
      </font>
   </Text>
   <Button fx:id="viewLevelStatsButton" layoutX="385.0" layoutY="648.0" mnemonicParsing="false"
           onAction="#onViewLevelStatsButtonClicked" text="View Level Stats">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <ChoiceBox fx:id="bpmSelector" layoutX="551.0" layoutY="508.0" prefHeight="25.0" prefWidth="70.0"/>
   <Text fill="WHITE" layoutX="482.0" layoutY="525.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select BPM"/>
   <Text fill="WHITE" layoutX="639.0" layoutY="301.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score:"
         textAlignment="RIGHT">
      <font>
         <Font size="49.0"/>
      </font>
   </Text>
   <Text fill="WHITE" layoutX="401.0" layoutY="383.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Level High Score:"
         textAlignment="RIGHT">
      <font>
         <Font size="49.0"/>
      </font>
   </Text>
